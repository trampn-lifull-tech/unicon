{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",

        "symfony/config": "~4.2.1",
        "symfony/proxy-manager-bridge": "~4.2.1",
        "symfony/yaml": "~4.2.1",
        "zendframework/zend-db": "~2.9.3",
        "doctrine/orm": "~2.6.3",
        "doctrine/doctrine-module": "~2.1.7",
        "jms/serializer": "~2.0.2",
        "m1/vars": "~1.1.2",
        "prooph/common": "~4.3.0"
    },
    "require-dev": {
        "laravel/tinker": "^1.0",
        "barryvdh/laravel-ide-helper": "~2.5.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Chaos\\SharedModule\\": "modules/shared/src",
            "Chaos\\CoreModule\\": "modules/core/src",
            "Chaos\\AppModule\\": "modules/app/src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "modules/shared/autoload/defines.php",
            "modules/shared/autoload/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
